  "timeframe": "5min",
  "symbol": "QQQ",
  "entry_conditions": [
    {
      "type": "gap_fade_short",
      "description": "Short QQQ when it gaps up more than 0.8% from previous close, with RSI overbought",
      "direction": "short",
      "indicators": ["vwap", "rsi", "ema20", "volume"],
      "conditions": [
        "gap_up_percentage > 0.8",
        "rsi > 70",
        "price > vwap",
        "volume > 1M"
      ],
      "time_window": "09:30-10:30",
      "gap_threshold": 0.8,
      "rsi_threshold": 70
    },
    {
      "type": "vwap_bounce_long",
      "description": "Long when QQQ pulls back to VWAP support with oversold RSI and volume spike",
      "direction": "long",
      "indicators": ["vwap", "rsi", "ema20", "volume"],
      "conditions": [
        "price within 0.3% of vwap",
        "rsi < 35",
        "volume > 1.5M",
        "price above ema20"
      ],
      "time_window": "09:45-15:30",
      "vwap_tolerance": 0.3,
      "rsi_oversold": 35
    }
  ],
  "exit_conditions": [
    {
      "type": "profit_target_short",
      "description": "Cover shorts when price returns to VWAP or hits 1R profit target",
      "direction": "cover_short",
      "conditions": [
        "price <= vwap OR profit >= 1R"
      ],
      "profit_target": 1.0,
      "vwap_target": true
    },
    {
      "type": "profit_target_long",
      "description": "Sell longs when price extends 1.2% above VWAP or hits 1.5R target",
      "direction": "exit_long",
      "conditions": [
        "price >= vwap * 1.012 OR profit >= 1.5R"
